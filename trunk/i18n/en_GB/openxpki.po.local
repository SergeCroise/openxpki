msgstr "Reset PIN - permission denied"
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_PIN_PERMISSION_DENIED_DESC"
msgstr "You are not allowed to reset the PIN of this token, because it contains certificates of other users: __USERS__."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_NEW_PIN_TITLE"
msgstr "Choose a new PIN"
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_NEW_PIN_DESC"
msgstr "Please choose a new PIN and confirm it when asked."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_ERROR_LIBRARY_ERROR"
msgstr "The control could not load the PKCS#11 library. Please contact PKI support for help."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_ERROR_PKCS11_INIT_FAILED"
msgstr "The initialization of the PKCS#11 library failed. Please contact PKI support for help."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_ERROR_TOKEN_ID_FAILURE"
msgstr "The control could not read the token ID. Please contact PKI support for help."
