msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_DELETE_CONFIRM_TITLE"
msgstr "Confirm token deletion"
msgid "I18N_OPENXPKI_HTML_MENU_RESET_TOKEN"
msgstr "Reset Token"
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_DELETE_CONFIRM_DESC"
msgstr "Are you sure that you want to delete the certificates and keys stored on your token? You will no longer have access to the private keys and certificates currently stored on the token."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_DELETE_CONFIRM_YES"
msgstr "Yes, I want to delete the certificates and keys on the token and set a new PIN."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_DELETE_CONFIRM_NO"
msgstr "No, I do not want to delete the certificates and keys stored on the token."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_RETRIEVING_TOKEN_ID_TITLE"
msgstr "Retrieving token ID ..."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_RETRIEVING_TOKEN_ID_DESC"
msgstr "The token ID is currently being retrieved from your token."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_SUCCESSFULLY_RESET_PIN_TITLE"
msgstr "PIN successfully reset"
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_SUCCESSFULLY_RESET_PIN_DESC"
msgstr "Your PIN has been reset successfully. You can now continue to use your token with the newly set PIN."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_SUCCESSFULLY_DELETED_TITLE"
msgstr "Token deletion successful"
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_SUCCESSFULLY_DELETED_DESC"
msgstr "The contents of your token have been deleted and your smartcard PIN has been reset successfully."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_PIN_RESET_ERROR_TITLE"
msgstr "PIN reset error"
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_PIN_RESET_ERROR_DESC"
msgstr "An error has occured while resetting the PIN. Please refer below for details on the error and possible solutions."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_DELETE_ERROR_TITLE"
msgstr "Token deletion error"
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_DELETE_ERROR_DESC"
msgstr "An error has occured while deleting the contents from your token or resetting the PIN. Please refer below for details on the error and possible solutions."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_DELETE_NEW_PIN_TITLE"
msgstr "Choose a new PIN"
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_DELETE_NEW_PIN_DESC"
msgstr "The contents of your token have been deleted. Please choose a new PIN when asked."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_ERROR_PUK_WORKFLOW_NOT_FOUND_TITLE"
msgstr "Error: PUK workflow not found"
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_ERROR_PUK_WORKFLOW_NOT_FOUND_DESC"
msgstr "The workflow for your PUK has not been found. This is an internal error - please contact PKI support for help."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_ERROR_PUK_RETRIEVE_FAILED_TITLE"
msgstr "Error: PUK retrieve failed"
