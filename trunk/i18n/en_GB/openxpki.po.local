msgstr "The user PIN could not be changed. Please contact PKI support for help."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_ERROR_WRONG_PIN_ERROR"
msgstr "You have entered an incorrect PIN."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_ERROR_UNKNOWN_PLUGIN_COMMAND"
msgstr "Internal error: Unknown plugin command. Please contact PKI support for help."
