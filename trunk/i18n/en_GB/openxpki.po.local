msgstr "The PUK could not be retrieved from the workflow. This is an internal error - please contact PKI support for help."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_ERROR_PUK_AES_ENCRYPTION_FAILED_TITLE"
msgstr "Error: PUK encryption failed"
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_ERROR_PUK_AES_ENCRYPTION_FAILED_DESC"
msgstr "The encryption of your PUK failed. This is an internal error - please contact PKI support for help."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_DELETE_OR_RESET_TITLE"
msgstr "Reset Token"
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_DELETE_OR_RESET_DESC"
msgstr "You can either reset your smart card PIN or set a new smart card PIN and delete the certificates and keys on your token."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_RESET_PIN_INTRO"
msgstr "In case you have forgotten your smart card PIN you can reset the PIN while maintaining the data (certificate and keys) on your token."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_DELETE_INTRO"
msgstr "In case an existing token has been transferred from another user, you have to delete the certificates and keys."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_DELETE"
msgstr "Reset Smartcard PIN and Delete Certificate and Keys"
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_RESET_PIN"
msgstr "Reset Smartcard PIN"
msgid "I18N_OPENXPKI_HTML_MASON_RESET_TOKEN_IE_AND_ACTIVEX_NEEDED"
msgstr "To access your token you need to use Internet Explorer and have the updated RSA Authentication Client 2.0.1 software package (APP-RSA_SID800_DRIVER_UTILITY-201-002) installed. Please contact your local helpdesk."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_READING_CERTIFICATES_TITLE"
msgstr "Reading certificates ..."
msgid "I18N_OPENXPKI_CLIENT_HTML_MASON_RESET_TOKEN_READING_CERTIFICATES_DESC"
msgstr "The certificates are currently being read from your token."
